{
  "name": "github-gatsby-page",
  "description": "Yet Another Github Page",
  "version": "1.0.0",
  "author": "Leonardo Quevedo",
  "homepage": "./",
  "dependencies": {
    "gatsby": "^2.2.9",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sass": "^2.0.11",
    "node-sass": "^4.11.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop --port 3000",
    "build": "gatsby build",
    "clear": "rm -rf *.js.map *.eot *-map.json *.stats.json *.woff* *.jpg *.svg *.css *.ttf *.html workbox-* offline-* d 404 page-2 *.webmanifest",
    "copy": "cp -rf public/* ./",
    "deploy": "npm run clear && npm run build && npm run copy && node .bin/deploy",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "gh-pages": "^2.0.1",
    "prettier": "^1.14.2",
    "shelljs": "^0.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leopq/leopq.github.io"
  }
}
